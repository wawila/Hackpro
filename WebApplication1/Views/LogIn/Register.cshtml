@model WebApplication1.Models.UserModel
@{
    Layout = null;
    ViewBag.Title = "Register";
}
<!DOCTYPE html>
<html>
<head>
    <title>HackPro Register</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/iCheck/skins/flat/_all.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/DataTables/media/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//bower_components/select2/dist/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//vendor/css/dataTables.bootstrap.css">
    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin//css/themes.css">
</head>
<body class="flat-blue login-page Rbody">
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="container">
        <div class="login-box">
            <div>
                <div class="login-form row">
                    <div class="col-sm-12 text-center login-header">
                        <i class="login-logo fa fa-connectdevelop fa-5x"></i>
                        <h4 class="login-title">HackPro</h4>
                        <h5 class="login-title">Register</h5>
                    </div>
                    <div class="col-sm-12">
                        <div class="login-body">
                            <div class="progress hidden" id="login-progress">
                                <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                                    Creando...
                                </div>
                            </div>
                            <form>
                                <div class="control">
                                    <span class="fa fa-at" aria-hidden="true"><strong> Correo Electronico</strong></span>
                                    @Html.EditorFor(model => model.Correo, new { htmlAttributes = new { @class = "form-control", @type = "email", @placeholder = "Correo@Direc.com" } })
                                    @Html.ValidationMessageFor(model => model.Correo, "", new { @class = "text-danger" })
                                    @Html.ValidationMessage("ErrorC")
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Nombre De Usuario</strong></span>
                                    @Html.EditorFor(model => model.UserName, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "Usuario" } })
                                    @Html.ValidationMessageFor(model => model.UserName, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-shield" aria-hidden="true"><strong> Contraseña</strong></span>
                                    @Html.EditorFor(model => model.Password, new { htmlAttributes = new { @class = "form-control", @type = "password", @placeholder = "Constraseña" } })
                                    @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-shield" aria-hidden="true"><strong> Confirmar Contraseña</strong></span>
                                    @Html.EditorFor(model => model.ConfirmPassword, new { htmlAttributes = new { @class = "form-control", @type = "password", @placeholder = "Confirmar Contraseña" } })
                                    @Html.ValidationMessageFor(model => model.ConfirmPassword, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Nombres</strong></span>
                                    @Html.EditorFor(model => model.PNombre, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "1er Nombre" } })
                                    @Html.ValidationMessageFor(model => model.PNombre, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    @Html.EditorFor(model => model.SNombre, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "2do Nombre" } })
                                    @Html.ValidationMessageFor(model => model.SNombre, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-user" aria-hidden="true"><strong> Apellido</strong></span>
                                    @Html.EditorFor(model => model.PApellido, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "1er Apellido" } })
                                    @Html.ValidationMessageFor(model => model.PApellido, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    @Html.EditorFor(model => model.SApellido, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "2do Apellido" } })
                                    @Html.ValidationMessageFor(model => model.SApellido, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-calendar" aria-hidden="true"><strong> Fecha Nacimiento</strong></span>
                                    @Html.EditorFor(model => model.FechaN, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "DD/MM/AAAA" } })
                                    @Html.ValidationMessageFor(model => model.FechaN, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-phone" aria-hidden="true"><strong> Telefono</strong></span>
                                    @Html.EditorFor(model => model.Celular, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "99999999" } })
                                    @Html.ValidationMessageFor(model => model.Celular, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-venus-mars" aria-hidden="true"><strong> Genero</strong></span>
                                    @Html.EditorFor(model => model.Genero, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "M/F" } })
                                    @Html.ValidationMessageFor(model => model.Genero, "", new { @class = "text-danger" })
                                </div>
                                <div class="control">
                                    <span class="fa fa-suitcase" aria-hidden="true"><strong> Ocupacion</strong></span>
                                    @Html.EditorFor(model => model.Ocupacion, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "Ocupacion" } })
                                    @Html.ValidationMessageFor(model => model.Ocupacion, "", new { @class = "text-danger" })
                                </div>

                                <div class="login-button text-center">
                                    <input type="submit" class="btn btn-primary" value="Crear">
                                </div>
                            </form>
                        </div>
                        <div class="login-footer">
                            <span class="text-right"><a href="@Url.Action("LogIn","LogIn")" class="color-white">Ya Tienes Una Cuenta</a></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Javascript Libs -->
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/chartjs/Chart.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/bootstrap-switch/dist/js/bootstrap-switch.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/iCheck/icheck.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/matchHeight/jquery.matchHeight-min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/DataTables/media/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//bower_components/select2/dist/js/select2.full.min.js"></script>

    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//vendor/js/dataTables.bootstrap.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//vendor/js/ace/ace.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//vendor/js/ace/mode-html.js"></script>
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//vendor/js/ace/theme-github.js"></script>
    <!-- Javascript -->
    <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin//js/app.js"></script>
}
</body>
</html>
