                <li><a href="#" class="icon fa-google-plus"><span class="label">Google+</span></a></li>
                <li><a href="#" class="icon fa-dribbble"><span class="label">Dribbble</span></a></li>
                <li><a href="#" class="icon fa-pinterest"><span class="label">Pinterest</span></a></li>
                <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
            </ul>


            <!-- Menu -->
            <ul class="menu">
                <li>&copy; Untitled</li>
                <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
            </ul>

        </footer>
        *@

        <!-- Scripts -->
        <script src="~/Content/TDashboard/assets/js/jquery.min.js"></script>
        <script src="~/Content/TDashboard/assets/js/jquery.poptrox.min.js"></script>
        <script src="~/Content/TDashboard/assets/js/jquery.scrolly.min.js"></script>
        <script src="~/Content/TDashboard/assets/js/jquery.scrollex.min.js"></script>
        <script src="~/Content/TDashboard/assets/js/skel.min.js"></script>
        <script src="~/Content/TDashboard/assets/js/util.js"></script>
        <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
        <script src="~/Content/TDashboard/assets/js/main.js"></script>


</body>
</html>
