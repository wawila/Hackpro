
@{
    ViewBag.Title = "ListaProyectos";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/iCheck/skins/flat/_all.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/DataTables/media/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/select2/dist/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/vendor/css/dataTables.bootstrap.css">
    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/css/themes.css">
</head>
<body class="flat-blue">
    <div class="app-container">
        <div class="row content-container">
            <!-- Main Content -->
            <div class="container-fluid">
                <div class="side-body">
                    <div class="page-title">
                        <span class="title"></span>
                        <div class="description"></div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="card">
                                <div class="card-header">
                                    <div class="card-title">
                                        <div class="title">Lista de Proyectos</div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <table class="datatable table table-striped" cellspacing="0" width="100%">
                                        <thead>
                                            <tr>
                                                <th> ID </th>
                                                <th> Nombre Proyecto </th>
                                                <th> ID Evento </th>
                                                <th> ID Equipo </th>
                                                <th> Url </th>
                                                <th> Git </th>
                                                <th> Estatus </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @Html.Raw(ViewBag.proyect)
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div>
            <!-- Javascript Libs -->
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/jquery/dist/jquery.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/chartjs/Chart.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap-switch/dist/js/bootstrap-switch.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/iCheck/icheck.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/matchHeight/jquery.matchHeight-min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/DataTables/media/js/jquery.dataTables.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/select2/dist/js/select2.full.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/dataTables.bootstrap.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/ace.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/mode-html.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/theme-github.js"></script>
            <!-- Javascript -->
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/js/app.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/js/index.js"></script>
        </div>
    </div>
</body>
</html>





