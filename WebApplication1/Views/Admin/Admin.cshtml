@model WebApplication1.Models.LoginModel
@{
    Layout = null;
    ViewBag.Title = "#HackPro";
}
<!DOCTYPE html>
<html>
<head>
    <title>#HackPro</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300,400' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
    <!-- CSS Libs -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/iCheck/skins/flat/_all.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/DataTables/media/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/bower_components/select2/dist/css/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/vendor/css/dataTables.bootstrap.css">
    <!-- CSS App -->
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/FlatAdmin/flat-admin/css/themes.css">
</head>
<body class="flat-blue">
    <div class="app-container">
        <div class="row content-container">
            <nav class="navbar navbar-default navbar-fixed-top navbar-top">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-expand-toggle">
                            <i class="fa fa-bars icon"></i>
                        </button>
                        <ol class="breadcrumb navbar-breadcrumb">
                            <li class="active">Dashboard</li>
                        </ol>
                        <button type="button" class="navbar-right-expand-toggle pull-right visible-xs">
                            <i class="fa fa-th icon"></i>
                        </button>
                    </div>
                    <ul class="nav navbar-nav navbar-right">
                        <button type="button" class="navbar-right-expand-toggle pull-right visible-xs">
                            <i class="fa fa-times icon"></i>
                        </button>
                        <li class="dropdown profile">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"> @Html.Raw(ViewBag.user) <span class="caret"></span></a>
                            <ul class="dropdown-menu animated fadeInDown">
                                <li class="profile-img">
                                    <img src="~/Content/FlatAdmin/flat-admin/img/profile/picjumbo.com_HNCK4153_resize.jpg" class="profile-img">
                                </li>
                                <li>
                                    <div class="profile-info">
                                        <h4 class="username">@Html.Raw(ViewBag.Name)</h4>
                                        <p>@Html.Raw(ViewBag.correo)</p>
                                        <div class="btn-group margin-bottom-2x" role="group">
                                            <button type="button" class="btn btn-default"><i class="fa fa-user"></i> Perfil</button>
                                            <button type="button" class="btn btn-default"><a href="@Url.Action("LogIn", "LogIn")"class="fa fa-sign-out"> Desconectarse </a> </button>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
            <div class="side-menu">
                <nav class="navbar navbar-default" role="navigation">
                    <div class="side-menu-container">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="Dashboard" target="ventana">
                                <div class="icon fa fa-paper-plane"></div>
                                <div class="title">#HackPro</div>
                            </a>
                            <button type="button" class="navbar-expand-toggle pull-right visible-xs">
                                <i class="fa fa-times icon"></i>
                            </button>
                        </div>
                        <ul class="nav navbar-nav">
                            <li class="active">
                                <a href="Dashboard" target="ventana">
                                    <span class="icon fa fa-tachometer"></span><span class="title">Tablero</span>
                                </a>
                            </li>
                            <li class="panel panel-default dropdown">
                                <a data-toggle="collapse" href="#dropdown-element">
                                    <span class="fa fa-users"></span><span class="title">Usuarios</span>
                                </a>
                                <!-- Dropdown level 1 -->
                                <div id="dropdown-element" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a href="ListaUsuarios" target="ventana">Lista de Usuarios</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                            <li class="panel panel-default dropdown">
                                <a data-toggle="collapse" href="#dropdown-cat">
                                    <span class="fa fa-list-ul"></span><span class="title">Categoria</span>
                                </a>
                                <!-- Dropdown level 2 -->
                                <div id="dropdown-cat" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a href="ListaCategoria" target="ventana">Lista De Categorias</a>
                                            </li>
                                            <li>
                                                <a href="CatEvent" target="ventana"> Crear Categoria </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                            <li class="panel panel-default dropdown">
                                <a data-toggle="collapse" href="#dropdown-equipo">
                                    <span class="fa fa-sitemap"></span><span class="title">Equipos</span>
                                </a>
                                <!-- Dropdown level 3 -->
                                <div id="dropdown-equipo" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a href="NewTeam" target="ventana"> Crear Equipo </a>
                                            </li>
                                            <li>
                                                <a href="ListaEquipos" target="ventana">Lista de Equipos</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>

                            <li class="panel panel-default dropdown">
                                <a data-toggle="collapse" href="#dropdown-proyect">
                                    <span class="fa fa-code"></span><span class="title">Proyecto</span>
                                </a>
                                
                                <!-- Dropdown level 4 -->
                                <div id="dropdown-proyect" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a href="NewProyect" target="ventana"> Crear Proyecto </a>
                                            </li>
                                            <li>
                                                <a href="ListaProyectos" target="ventana">Lista de Proyectos</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                            <li class="panel panel-default dropdown">
                                <a data-toggle="collapse" href="#dropdown-table">
                                    <span class="fa fa-calendar"></span><span class="title">Eventos</span>
                                </a>
                                
                                <!-- Dropdown level 5 -->
                                <div id="dropdown-table" class="panel-collapse collapse">
                                    <div class="panel-body">
                                        <ul class="nav navbar-nav">
                                            <li>
                                                <a href="NewEvents" target="ventana"> Crear Evento </a>
                                            </li>
                                            <li>
                                                <a href="ListaEventos" target="ventana">Lista de Eventos</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <!-- /.navbar-collapse -->
                </nav>
            </div>
            <!-- Main Content -->
            <div class="container-fluid">
                <div class="side-body padding-top">
                    <div class="row">
                        <iframe style="width: 100%;" height="575px" name="ventana" src="Dashboard"></iframe>
                    </div>
                </div>
            </div>
        </div>
        <footer class="app-footer">
            <div class="wrapper">
                <span class="pull-right">2.0 <a href="#"><i class="fa fa-long-arrow-up"></i></a></span> © 2015 Copyright.
            </div>
        </footer>
        <div>
            <!-- Javascript Libs -->
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/jquery/dist/jquery.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/chartjs/Chart.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/bootstrap-switch/dist/js/bootstrap-switch.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/iCheck/icheck.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/matchHeight/jquery.matchHeight-min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/DataTables/media/js/jquery.dataTables.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/bower_components/select2/dist/js/select2.full.min.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/dataTables.bootstrap.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/ace.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/mode-html.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/vendor/js/ace/theme-github.js"></script>
            <!-- Javascript -->
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/js/app.js"></script>
            <script type="text/javascript" src="~/Content/FlatAdmin/flat-admin/js/index.js"></script>
        </div>
    </div>
</body>
</html>